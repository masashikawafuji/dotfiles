{
  "explorer.icon.enableNerdfont": true,
  "explorer.file.hiddenRules": {
    "extensions": [
      "o",
      "a",
      "obj",
      "pyc"
    ],
    "filenames": [],
    "patternMatches": [
      "^\\.git$"
    ]
  },
  "tsserver.disableAutomaticTypeAcquisition": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "graphql",
    "css",
    "scss",
    "html",
    "yaml"
  ]
}
